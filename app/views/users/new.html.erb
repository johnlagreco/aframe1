<body id="signupbod">
<%= render 'layouts/navigation' %>
<div id="signupform">	
<%= form_for @user do |f| %>
  <h1><%= f.text_field :coname, :placeholder => "Business Name" %><br>
  <%= f.text_field :ownername, :placeholder => "Your Full Name" %><br>
  <%= f.text_field :address, :placeholder => "Business Address" %><br>
  <%= f.text_field :city, :placeholder => "City" %><br>
  <%= f.text_field :state, :placeholder => "State" %><br>
  <%= f.text_field :zip, :placeholder => "Zip Code" %><br>
  <%= f.password_field :password, :placeholder => "Password" %>
  <br>
  <%= f.submit 'Sign Up'%></h1>
<% end %>
</div>
</body>